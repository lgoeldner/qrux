(defun! fold (init fn over)
	(if (empty? over)
		init
		(fold 
			(fn init (car over))
			fn
			(cdr over))))

(defun! reduce (fn over)
	(fold (car over) fn (cdr over)))

(defun! zip (a b (acc ()))
	(if (or (empty? a) (empty? b))
		(rev acc)
		(zip 
			(cdr a) (cdr b)
			(cons (list (car a) (car b)) acc))))

(export! '(fold reduce zip enum ))