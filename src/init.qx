(defmacro! defun! (ident args body)
	`(def! ,ident (fn* ,args ,body)))

(defmacro! when (cond &body)
	`(if ,cond (do ~body)))


(defun! not (bool) 
	(if bool 
		false 
		true))

(defun! rep (str)
	(eval
		(read-string str)))

(defun! loadf (path)
	(rep
		(str "(do"
			(slurp path) ")")))

